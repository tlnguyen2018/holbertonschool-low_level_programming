#include "holberton.h"
/**
 * leet - converse letter to number
 * @s: input string
 * Return: changed string
 */
char *leet(char *s)
{

	int index1;
	int index2;
	char *original = "aeotlAEOTL";
	char *new = "4307143071";

	for (index1 = 0; s[index1] != '\0'; index1++)
	{
		for (index2 = 0; original[index2] != '\0'; index2++)
		{
			if (s[index1] == orginal[index2]
			{
				s[index1] = new[index1];
			}
		}
	}
	return (s);
}
