#include "holberton.h"

/**
*string_toupper - lowercase to uppercase
*Description: lower to upper
*@*: pointer
*Description'': meme
*Return: new char string
*/

char *string_toupper(char*)

{
	int index = 0;
	char s;

	while (s[index] != '\0')
	{
		if (s[index] >= 'a' && s[index] <= 'z')
			s[index] = s[index] - 32;
		index++;
	}
	return (s);
}
