#ifndef _DOG_H
#define _DOG_H

/**
 * struct dog - new dog structure
 * @name: name
 * @age: age
 * @owner: owner
 *
 * Description: dog type
 */
typedef struct dog
{

	char *name;
	float age;
	char *owner;
} my_dog;

int _putchar(char c);
void init_dog(struct dog *d, char *name, float age, char *owner);
void print_dog(struct dog *d);
dog_t *new_dog(char *name, float age, char *owner);
void free_dog(dog_t *d);

#endif
