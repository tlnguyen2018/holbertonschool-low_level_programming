#include "holberton.h"
#include <stdio.h>

/**
 *main - entry point
 *Description: print out numbers of argument
 *@agrc: number of arguments
 *@agrv: array of strings
 *Return: always 0
 */
int main(int agrc, char *agrv[])
{
	int index = 0;
	(void) agrv;

	for (index = 0; index < agrc; index++)
	{
		printf("%d\n", agrc - 1);
	}
	return (0);
}
